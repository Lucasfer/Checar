<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufpi.es.checar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Visao.TelaPrincipal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Visao.TelaNovoCNH"  android:parentActivityName="br.ufpi.es.checar.Visao.TelaPrincipal">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.ufpi.es.checar.Visao.TelaPrincipal" />


        </activity>
    </application>

</manifest>
